{
    "AWSEBDockerrunVersion": 2,
    "ContainerDefinitions": [
        {
            "Name": "client",
            "Image": "jaynejacobs/multi-client",
            "hostname": "client",
            "Essential": false,
            "memory": 128
        },
        {
            "Name": "server",
            "Image": "jaynejacobs/multi-server",
            "hostname": "api",
            "Essential": false,
            "memory": 128
        },
        {
            "Name": "worker",
            "Image": "jaynejacobs/multi-worker",
            "Essential": false,
            "memory": 128
        },
        {
            "Name": "nginxr",
            "Image": "jaynejacobs/multi-nginx",
            "Essential": true,
            "portMappins": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client",
                "server"
            ],
            "memory": 128
        }
    ]
}